{
  "name": "tcstk-base-app",
  "version": "1.1.0",
  "scripts": {
    "tcli": "npm install gulp-cli -g && npm install -g @tibco-tcstk/cloud-cli && tcli",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve_eu": "ng serve --proxy-config proxy.conf.prod.eu.json --ssl true --source-map=true --aot",
    "serve_us": "ng serve --proxy-config proxy.conf.prod.us.json --ssl true --source-map=true --aot",
    "serve_au": "ng serve --proxy-config proxy.conf.prod.au.json --ssl true --source-map=true --aot",
    "build_prod_hash": "ng build --prod --base-href /webresource/apps/tcstk-base-app/index.html --deploy-url /webresource/apps/tcstk-base-app/ && cd ./dist/tcstk-base-app/ && zip -r ./app.zip ./* && cd ../..",
    "build_prod": "ng build --prod --base-href /webresource/apps/tcstk-base-app/ --deploy-url /webresource/apps/tcstk-base-app/ && cd ./dist/tcstk-base-app/ && zip -r ./app.zip ./* && cd ../.."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@tibco-tcstk/tc-core-lib": "^1.2.0",
    "@tibco-tcstk/tc-forms-lib": "^1.2.0",
    "@tibco-tcstk/tc-liveapps-lib": "^1.2.0",
    "angular6-json-schema-form": "^8.0.0",
    "chart.js": "^2.9.3",
    "chartjs-plugin-datalabels": "^0.7.0",
    "chartjs-plugin-doughnutlabel": "^2.0.3",
    "core-js": "^3.6.2",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "ng2-charts": "2.3.0",
    "ngx-color-picker": "^8.2.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "brace": "^0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular/cli": "^8.3.21",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "~8.2.14",
    "@tibco-tcstk/component-template": "^1.2.0",
    "@types/node": "^12.12.24",
    "codelyzer": "^5.2.1",
    "ts-node": "~8.5.2",
    "tslint": "~5.20.1",
    "typescript": "~3.4.5"
  }
}
