{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@TIBCOSoftware/tc-liveapps-lib": [
        "dist/tibco-tcstk/tc-liveapps-lib"
      ],
      "@TIBCOSoftware/tc-liveapps-lib/*": [
        "dist/tibco-tcstk/tc-liveapps-lib/*"
      ],
      "@TIBCOSoftware/tc-core-lib": [
        "dist/tibco-tcstk/tc-core-lib"
      ],
      "@TIBCOSoftware/tc-core-lib/*": [
        "dist/tibco-tcstk/tc-core-lib/*"
      ],
      "@TIBCOSoftware/tc-forms-lib": [
        "dist/tibco-tcstk/tc-forms-lib"
      ],
      "@TIBCOSoftware/tc-forms-lib/*": [
        "dist/tibco-tcstk/tc-forms-lib/*"
      ],
      "@TIBCOSoftware/tc-messaging-lib": [
        "dist/tibco-tcstk/tc-messaging-lib"
      ],
      "@TIBCOSoftware/tc-messaging-lib/*": [
        "dist/tibco-tcstk/tc-messaging-lib/*"
      ],
      "@TIBCOSoftware/tc-spotfire-lib": [
        "dist/tibco-tcstk/tc-spotfire-lib"
      ],
      "@TIBCOSoftware/tc-spotfire-lib/*": [
        "dist/tibco-tcstk/tc-spotfire-lib/*"
      ],
      "@TIBCOSoftware/tc-events-lib": [
        "dist/tibco-tcstk/tc-events-lib"
      ],
      "@TIBCOSoftware/tc-events-lib/*": [
        "dist/tibco-tcstk/tc-events-lib/*"
      ],
      "@TIBCOSoftware/tc-ag-grid": [
        "dist/tibco-tcstk/tc-ag-grid"
      ],
      "@TIBCOSoftware/tc-ag-grid/*": [
        "dist/tibco-tcstk/tc-ag-grid/*"
      ],
      "@TIBCOSoftware/tc-scribe-lib": [
        "dist/tibco-tcstk/tc-scribe-lib"
      ],
      "@TIBCOSoftware/tc-scribe-lib/*": [
        "dist/tibco-tcstk/tc-scribe-lib/*"
      ],
      "@TIBCOSoftware/tc-primeng-lib": [
        "dist/tibco-tcstk/tc-primeng-lib"
      ],
      "@TIBCOSoftware/tc-primeng-lib/*": [
        "dist/tibco-tcstk/tc-primeng-lib/*"
      ],
      "@TIBCOSoftware/tc-tsc-lib": [
        "dist/tibco-tcstk/tc-tsc-lib"
      ],
      "@TIBCOSoftware/tc-tsc-lib/*": [
        "dist/tibco-tcstk/tc-tsc-lib/*"
      ],
      "@TIBCOSoftware/tc-tcmd-lib": [
        "dist/tibco-tcstk/tc-tcmd-lib"
      ],
      "@TIBCOSoftware/tc-tcmd-lib/*": [
        "dist/tibco-tcstk/tc-tcmd-lib/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": true,
    "compilationMode": "partial"
  }
}
