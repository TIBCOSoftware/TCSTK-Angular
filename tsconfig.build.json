{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "tc-liveapps-lib": [
        "dist/tc-liveapps-lib"
      ],
      "tc-liveapps-lib/*": [
        "dist/tc-liveapps-lib/*"
      ],
      "tc-core-lib": [
        "dist/tc-core-lib"
      ],
      "tc-core-lib/*": [
        "dist/tc-core-lib/*"
      ],
      "tc-forms-lib": [
        "dist/tc-forms-lib"
      ],
      "tc-forms-lib/*": [
        "dist/tc-forms-lib/*"
      ],
      "tc-handsontable-lib": [
        "dist/tc-handsontable-lib"
      ],
      "tc-handsontable-lib/*": [
        "dist/tc-handsontable-lib/*"
      ],
      "tc-process-discovery-lib": [
        "dist/tc-process-discovery-lib"
      ],
      "tc-process-discovery-lib/*": [
        "dist/tc-process-discovery-lib/*"
      ],
      "tc-spotfire-play-lib": [
         "dist/tc-spotfire-play-lib"
      ],
      "tc-spotfire-play-lib/*": [
        "dist/tc-spotfire-play-lib/*"
      ],
      "tc-check-workflow-monitor-lib": [
        "dist/tc-check-workflow-monitor-lib"
      ],
      "tc-check-workflow-monitor-lib/*": [
        "dist/tc-check-workflow-monitor-lib/*"
      ],
      "tc-spotfire-lib": [
        "dist/tc-spotfire-lib"
      ],
      "tc-spotfire-lib/*": [
        "dist/tc-spotfire-lib/*"
      ]
    }
  }
}
